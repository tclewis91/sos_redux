<p id="notice"><%= notice %></p>

<h1>Listing First Aids</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_aids.each do |first_aid| %>
      <tr>
        <td><%= link_to 'Show', first_aid %></td>
        <td><%= link_to 'Edit', edit_first_aid_path(first_aid) %></td>
        <td><%= link_to 'Destroy', first_aid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New First aid', new_first_aid_path %>
